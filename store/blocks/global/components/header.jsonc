      "header-row#3header.mobile"
    ]
  },
  "header-row#2header.mobile":{
    "children":[
      "drawer#header__mobile--menu",//menu responsive
      "modal-trigger#header__section--2",//modal add ubicacion
      "logo#header__section--logo",
