      "minicart.v2"
    ],
    "props":{
      "blockClass":"header__mobile--top"
    }
  },
  "drawer#header__mobile--menu":{// menu responsive
      "blocks":[
        "drawer#trigger"
    ]
  },
  "drawer#trigger":{
