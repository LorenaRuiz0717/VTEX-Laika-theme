{
    "flex-layout.row#product__page--tabs": {
        "title": "product container page tabs",
        "children": ["tab-layout#product__tabs"],
        "props":{
          "blockClass":"product__page--container-tabs-description-product"
        }
      },
      "tab-layout#product__tabs": {
        "title": "product page container tabs",
        "children": [
          "tab-list#product__page--tabs-list",
          "tab-content#product__tabs-content"
        ],
        "props":{
          "blockClass":"product__page--tabs-description-product"
        }
      },
      "tab-list#product__page--tabs-list": {
        "title": "product page tabs list",
        "children": [
          "tab-list.item#tabs__list--benefist",
          "tab-list.item#tabs__list--characteristic",
          "tab-list.item#tabs__list--description"
        ]
      },
      "tab-list.item#tabs__list--benefist": {
        "title": "product page tabs list benefist",
        "props": {
          "tabId": "benefist",
          "label": "Beneficios",
          "defaultActiveTab": true,
          "blockClass":"product__tabs_list--benefist"
        }
      },
      "tab-list.item#tabs__list--characteristic": {
        "title": "product page tabs list characteristic",
        "props": {
          "tabId": "characteristic",
          "label": "Caracteristicas",
          "defaultActiveTab": false,
          "blockClass":"product__tabs_list--characteristic"
        }
      },
      "tab-list.item#tabs__list--description": {
        "title": "product page tabs list description",
        "props": {
          "tabId": "description",
          "label": "Descripcion",
          "defaultActiveTab": false,
          "blockClass":"product__tabs_list--description"
        }
      },
      "tab-content#product__tabs-content": {
        "title": "product page tabs content",
        "children": [
          "tab-content.item#tabs__content--benefist",
          "tab-content.item#tabs__content--characteristic",
          "tab-content.item#tabs__content--description"
        ],
        "props":{
          "blockClass":"product__tabs_content"
        }
      },
