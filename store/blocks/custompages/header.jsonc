{
    "store.custom#header": {
      "title": "Header",
      "blocks": ["header-layout.desktop"
    //    "header-layout.mobile"
    ]
    },
    // "header.full": {
    //   "blocks": ["header-layout.desktop", "header-layout.mobile"]
